{
  "name": "appium_automation",
  "version": "1.0.0",
  "description": "cypress",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli cypress/report/*.* && del-cli cypress/report/mochawesome-report/*.* ",
    "cypress:run": "npx cypress run --browser chrome",
    "merge": "npx mochawesome-merge cypress/report/mochawesome-report/*.json > cypress/report/output.json",
    "generate_mochawesome_report": "npx marge ./cypress/report/output.json --reportDir cypress/report/abc.html"
  },
  "author": "pawan",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^5.6.0",
    "cypress-xpath": "^1.6.1",
    "del-cli": "^3.0.1",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "npm-run-all": "^4.1.5"
  }
}
